<!DOCTYPE html>
<html>
  <head>
    <title><%= title %></title>
    <style>
       #map {
        height: 600px;
        width: 100%;
       }
    </style>
  </head>
  <body>
    
    <h3>My Google Maps Demo</h3>
    <div id="map"></div>
    <script>

    function initMap() {
        var Center = {lat: <%= result.solutions[0].items[0].latitude %>, lng: <%= result.solutions[0].items[0].longitude %>};
        var map = new google.maps.Map(document.getElementById('map'), {
          zoom: 14,
          center: Center
        });

        var minLat = Center.lat, maxLat = Center.lat, minLng = Center.lng, maxLng = Center.lng;
                
        <% for(var i=0; i<result.solutions[0].items.length; i++){ %>
            var lat = <%= result.solutions[0].items[i].latitude %>, 
                lng = <%= result.solutions[0].items[i].longitude %>,
                id = <%= result.solutions[0].items[i].id %>; 
            minLat = Math.min(minLat, lat);
            maxLat = Math.max(maxLat, lat);

            minLng = Math.min(minLng, lng);
            maxLng = Math.max(maxLng, lng);

            var marker = new google.maps.Marker({
              position: {lat: lat, lng: lng},
              map: map,
              label: String(id)
              //icon: image
              //title: String(id)
            }); 
        <% } %>

        var mbrCenter = {lat: (minLat+maxLat)/2, lng: (minLng+maxLng)/2};
        minLat = mbrCenter.lat - <%= parameters.R.d1 %> / 2;
        minLng = mbrCenter.lng - <%= parameters.R.d2 %> / 2;
        
        var rectangle = new google.maps.Rectangle({
            strokeColor: '#FF0000',
            strokeOpacity: 0.8,
            strokeWeight: 2,
            fillColor: '#FF0000',
            fillOpacity: 0.35,
            map: map,
            bounds: {
              north: minLat,
              south: minLat + <%= parameters.R.d1 %>,
              east: minLng + <%= parameters.R.d2 %>,
              west: minLng
            }
        });
    }
    </script>
    <script async defer
    src="https://maps.googleapis.com/maps/api/js?key=AIzaSyC6hbSjCuN4EQsh34uhHO1C7gMQNdwBBg4&callback=initMap">
    </script>
    <!---->
  </body>
</html>